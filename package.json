{
  "name": "starter-angular2-with-webpack",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "tsc": "tsc -p ./ --outDir ./.tmp/es6",
    "babel": "babel .tmp/es6 --out-dir .tmp/es5",
    "webpack": "webpack .tmp/es5/index.js dist/bundle.js",
    "build": "npm run tsc && npm run babel && npm run webpack",
    "clean": "rm -rf .tmp dist && mkdir dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.9",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.13",
    "reflect-metadata": "^0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "zone.js": "^0.5.15"
  },
  "devDependencies": {
    "babel": "^6.5.2",
    "babel-cli": "^6.6.5",
    "babel-preset-es2015": "^6.6.0",
    "typescript": "^1.8.9",
    "webpack": "^1.12.14"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
